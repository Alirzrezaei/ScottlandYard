package logic;

/**
 *
 * @author Alireza
 */
public class SaveLoad {

    private LoadMisterX misterX;
    private LoadDetective detectives;
    private int whosTurn;
    private int currRoundNo;
    private boolean gameIsWon;

    public LoadMisterX getMisterX() {
        return misterX;
    }

    public LoadDetective getDetectives() {
        return detectives;
    }

    public int getWhosTurn() {
        return whosTurn;
    }

    public int getCurrRoundNo() {
        return currRoundNo;
    }

    public boolean isGameIsWon() {
        return gameIsWon;
    }

    public void setMisterX(LoadMisterX misterX) {
        this.misterX = misterX;
    }

    public void setDetectives(LoadDetective detectives) {
        this.detectives = detectives;
    }

    public void setWhosTurn(int whosTurn) {
        this.whosTurn = whosTurn;
    }

    public void setCurrRoundNo(int currRoundNo) {
        this.currRoundNo = currRoundNo;
    }

    public void setGameIsWon(boolean gameIsWon) {
        this.gameIsWon = gameIsWon;
    }

}
